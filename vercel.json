{
    "builds": [
        {
            "src": "./server.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "https://youtube-to-spotify-converter-git-master-siriiuss.vercel.app/index.html"
        }
  ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}