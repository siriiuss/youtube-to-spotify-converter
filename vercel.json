{
    "builds": [
        {
            "src": "./server.py",
            "use": "@vercel/python"
        }
    ],
      "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.example.com/:path*"
    }
  ],
    "routes": [
        {
            "src": "/index.html",
            "dest": "/"
        }
    ]
}